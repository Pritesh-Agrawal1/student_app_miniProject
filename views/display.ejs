<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Display</title>
</head>
<body>
    <h4>All Student List</h4>
    <table border="2">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Marks</td>
            <td>Operations</td>
        </tr>
        <% for(var i=0; i<student.length; i++){%>
            <tr>
                <td><%= student[i].id%></td>
                <td><%= student[i].name%></td>
                <td><%= student[i].marks%></td>
                <td><a href="deletestudent/<%= student[i].id%>">Delete</a></td>
                <td><a href="editstudent/<%= student[i].id%>">Edit</a></td>
            </tr>
        <% } %>
    </table>
</body>
</html>

